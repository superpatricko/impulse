								CUR_CUST."PRV_BAN",
								CUR_CUST."STATUS_ACTV_CODE",
								CUR_CUST."STATUS_ACTV_RSN_CODE",
								CUR_CUST."SUB_STATUS_DATE",
								CUR_CUST."SUB_STATUS",
								CUR_CUST."SUB_STATUS_LAST_ACT",
								CUR_CUST."SUB_STS_ISSUE_DATE",
								CUR_CUST."TAX_PROVINCE",
