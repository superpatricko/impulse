    ELSEIF CUR_CUST.FLAG_EXISTING_RECORD = 'I' THEN  
    	
    	BEGIN AUTONOMOUS TRANSACTION	          
--      Insert Records into the target NDB table INT_CUST	  
		INSERT INTO "NDB_COMMON"."IFRS_R1.COMMON_COMP.NDB_COMMON::OB_CUSTOMER"(
			   	  	"CUSTOMER_ID",
					"SUBSCRIBER_NO",
					"BUSINESS_ID",
