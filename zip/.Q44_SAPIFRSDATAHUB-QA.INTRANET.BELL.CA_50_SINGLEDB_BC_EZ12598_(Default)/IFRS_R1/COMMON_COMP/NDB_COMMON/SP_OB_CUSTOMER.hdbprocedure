							    CUR_CUST."START_SERVICE_DATE",
							    CUR_CUST."ACCOUNT_TYPE",
							    CUR_CUST."ACCOUNT_SUB_TYPE",
							    CUR_CUST."ADR_PROVINCE",
							    CUR_CUST."DEFAULT_SUB_MARKET",
							    CUR_CUST."RELATED_MARKET",
						        CUR_CUST."ADR_POSTAL", 
							    CUR_CUST."FIRST_NAME",
								CUR_CUST."LAST_BUSINESS_NAME",
								CUR_CUST."PRV_BAN",
								CUR_CUST."STATUS_ACTV_CODE",
								CUR_CUST."STATUS_ACTV_RSN_CODE",
								CUR_CUST."SUB_STATUS_DATE",
								CUR_CUST."SUB_STATUS",
								CUR_CUST."SUB_STATUS_LAST_ACT",
								CUR_CUST."SUB_STS_ISSUE_DATE",
								CUR_CUST."TAX_PROVINCE",
								CUR_CUST."EFF_DT", 				-- EFF_DT from source is populated in REFERENCE_EFFECTIVE_DT column in EH table (in case of Customer only) 
								CUR_CUST."REFERENCE_EXPIRY_DT",
