   END; 	  
   
   
   -----------------------------------------------------------------------------------------------   
    
-- FOR CLEARING THE VARIABLES

	IF ((LV_PREV_RECORD IS NULL) OR (CUR_CUST."CONCAT_PK" != LV_PREV_RECORD)) THEN
