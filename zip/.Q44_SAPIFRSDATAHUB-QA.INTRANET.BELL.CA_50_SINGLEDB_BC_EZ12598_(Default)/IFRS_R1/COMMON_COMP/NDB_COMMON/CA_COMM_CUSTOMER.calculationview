          <formula>if( isnull(&quot;MODIFIED_TS&quot;) and (not isnull(&quot;KUNNR_SID&quot;)) , NOW() , &quot;CC_MODIFIED_TS&quot;)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="SECONDDATE" id="CC_MODIFIED_TS" expressionLanguage="SQL">
          <formula>to_seconddate(to_varchar(&quot;MODIFIED_TS&quot;))</formula>
        </calculatedViewAttribute>
