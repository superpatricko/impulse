-- Update Landing Table IFRS_R1.LAND::P77_TRANS_HDR
UPDATE "LAND"."IFRS_R1.LAND::P77_TRANS_HDR" A
  FROM "LAND"."IFRS_R1.LAND::P77_TRANS_HDR" A 
 INNER JOIN #UPDATE_P77_SALES B 
    ON A."TRANS_ID"        = B."TRANSACTION_ID"
   AND A."CONTR_INCPTN"    = B."REC_EFFECTIVE_DT"
   SET A.DEV_TS           = LV_CUR_UTC
 WHERE A.DEV_TS IS NULL;

