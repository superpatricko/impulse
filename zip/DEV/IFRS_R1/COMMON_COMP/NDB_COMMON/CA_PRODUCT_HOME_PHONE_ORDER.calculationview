WHEN &quot;ITEM_GROUP_CODE&quot; = 'ACTIVITIES' THEN 'OTC'&#xD;
WHEN &quot;ITEM_TYPE&quot; = 'D' AND &quot;DISCOUNT_TYPE&quot; = '1' THEN 'UNO'&#xD;
WHEN &quot;ITEM_TYPE&quot; = 'D' AND &quot;DISCOUNT_TYPE&quot; = '2' THEN 'DUO'&#xD;
WHEN &quot;ITEM_TYPE&quot; = 'D' AND &quot;DISCOUNT_TYPE&quot; = '3' THEN 'TRIO'&#xD;
WHEN &quot;ITEM_TYPE&quot; = 'D' AND &quot;DISCOUNT_TYPE&quot; IS NULL THEN 'OTHER'&#xD;
ELSE 'OTHER' END</formula>
