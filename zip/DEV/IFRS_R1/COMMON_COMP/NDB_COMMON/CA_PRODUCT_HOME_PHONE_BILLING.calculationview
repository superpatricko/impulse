          <formula>case&#xD;
when &quot;PRODUCT_SERVICE_TYPE&quot; = 'S' and &quot;ITEM_TYPE&quot; = 'S' then 'OPT'&#xD;
when &quot;PRODUCT_SERVICE_TYPE&quot; = 'S' and &quot;ITEM_TYPE&quot; = 'P' then 'BASE_SRVC'&#xD;
when &quot;PRODUCT_SERVICE_TYPE&quot; = 'N' then 'OTC'&#xD;
when &quot;ITEM_TYPE&quot; = 'S' AND &quot;MDM_DISCOUNT_TYPE&quot; = '1' THEN 'UNO'&#xD;
when &quot;ITEM_TYPE&quot; = 'S' AND &quot;MDM_DISCOUNT_TYPE&quot; = '2' THEN 'DUO'&#xD;
when &quot;ITEM_TYPE&quot; = 'S' AND &quot;MDM_DISCOUNT_TYPE&quot; = '3' THEN 'TRIO'&#xD;
when &quot;ITEM_TYPE&quot; = 'S' AND &quot;MDM_DISCOUNT_TYPE&quot; IS NULL  THEN  'OTHER'&#xD;
ELSE 'OTHER'&#xD;
end</formula>
