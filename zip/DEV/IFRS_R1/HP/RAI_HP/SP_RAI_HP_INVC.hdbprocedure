                                                CUR_ORDER."CC_QUANTITY_UNIT",
                                                CUR_ORDER."CC_BILREL",
                                                CUR_ORDER."PRODUCT_START_DATE",
                                                CUR_ORDER."PRODUCT_END_DATE",
                                                CUR_ORDER."CC_NEGATIVE",
                                                CUR_ORDER."RECEIV_ACCOUNT",
