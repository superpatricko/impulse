PROCEDURE "NDB_COMMON"."IFRS_R1.COMMON_COMP.NDB_COMMON::SP_INT_KUNNR_UPDT"  
(OUT OT_STATUS NVARCHAR(1000))  
	LANGUAGE SQLSCRIPT 
	SQL SECURITY INVOKER 
	--DEFAULT SCHEMA <default_schema_name>
	--READS SQL DATA 
	AS
BEGIN
-- Procedure Title : SP_INT_KUNNR_UPDT
--
-- Created By      : BC_EZ07674(Suresh Konidala)          Date :02/12/2016
-- 
-- Procedure Description : This is a stored procedure designed to update KUNNR and ECC TS from COMM_CUST  
--						   table to INT_CUSTOMER table in NDB.
-- Project :Bell Canada
-- 
-- Release :R1/IFRS
--------------------------------------------------------------------------------
--   M O D I F I C A T I O N   H I S T O R Y
--------------------------------------------------------------------------------
-- Date        | User ID          | Modification History/Release
--------------------------------------------------------------------------------
--             |                  | 
--------------------------------------------------------------------------------
-- Description Of the changes 
--------------------------------------------------------------------------------
-- Modification Number          :<Assign Some Number> 
-- Description of Changes Made> :<Description of Changes>       
--------------------------------------------------------------------------------

--------------------------------------------------------------------------------------------------
-- Local Variables Declaration
--------------------------------------------------------------------------------------------------
DECLARE LV_CUR_UTC 				TIMESTAMP := CURRENT_TIMESTAMP;
DECLARE LV_ERROR_FLAG 			NVARCHAR(1) := 'N';
DECLARE LC_ERROR_TEXT 			NVARCHAR(120) 
            					DEFAULT 'OUT STATUS IS 1 : PROCEDURE 
            			IFRS_R1.COMMON_COMP.NDB_COMMON::SP_INT_KUNNR_UPDT UPDATE EXECUTION FAILED';
DECLARE LC_SUCCESS_TEXT 		NVARCHAR(120) 
								DEFAULT 'OUT STATUS IS 0 : PROCEDURE 
					IFRS_R1.COMMON_COMP.NDB_COMMON::SP_INT_KUNNR_UPDT UPDATE EXECUTION SUCCESSFUL';
					
--------------------------------------------------------------------------------------------------
-- Cursors Declaration
-------------------------------------------------------------------------------------------------- 
DECLARE CURSOR C_INT_KUNNR_UPDT FOR
	   SELECT
		 "PRM_CONTR_CUST",
		 "ALTN_CONTR_NUM",
		 "KUNNR_SID",
		 "ECC_TS"
		FROM "_SYS_BIC"."IFRS_R1.COMMON_COMP.NDB_COMMON/CA_INT_KUNNR_UPDT";
		
--------------------------------------------------------------------------------------------------
-- Local Temporary Tables Declaration
-------------------------------------------------------------------------------------------------- 
CREATE LOCAL TEMPORARY TABLE #UPDATE_EH_DELETION ("PRM_CONTR_CUST" NVARCHAR(35), "ALTN_CONTR_NUM" NVARCHAR(35));
--------------------------------------------------------------------------------------------------
-- Cursors Loop
--------------------------------------------------------------------------------------------------  

FOR INT_KUNNR AS C_INT_KUNNR_UPDT DO
	--  Define Exit Handler
	DECLARE EXIT HANDLER FOR SQLEXCEPTION 
	BEGIN AUTONOMOUS TRANSACTION 
	
	  -- Define Exit Handler
	  DECLARE EXIT HANDLER FOR SQLEXCEPTION 
	  BEGIN AUTONOMOUS TRANSACTION 	
	   
	     -- Do Nothing. Exception Handled to ensure SP does not get terminated 
	
 	  END;
	
   	  -- Insert record into error table
	  INSERT INTO "NDB_INTERNET"."IFRS_R1.INTERNET.NDB_INTERNET::INT_CUST_EH"
	   	  ("PRM_CONTR_CUST",
	   	  	"ALTN_CONTR_NUM", 
     		"KUNNR_SID",
     		"MODIFIED_TS",
			"ERROR_CODE",
			"TECHNICAL_MESSAGE",
			"ERROR_CREATE_TS")
	  VALUES( INT_KUNNR."PRM_CONTR_CUST",
	  		  INT_KUNNR."ALTN_CONTR_NUM",
     		  INT_KUNNR."KUNNR_SID", 
     		  INT_KUNNR."ECC_TS",
			  ::SQL_ERROR_CODE,
			  ::SQL_ERROR_MESSAGE,
			  LV_CUR_UTC); 
			   	    
	  LV_ERROR_FLAG := 'Y';    
	END;
	
	UPDATE "NDB_INTERNET"."IFRS_R1.INTERNET.NDB_INTERNET::INT_CUST"
	SET "KUNNR_SID" = INT_KUNNR."KUNNR_SID", "MODIFIED_TS" = INT_KUNNR."ECC_TS"
	WHERE "PRM_CONTR_CUST" = INT_KUNNR."PRM_CONTR_CUST"
	 AND  "ALTN_CONTR_NUM" = INT_KUNNR."ALTN_CONTR_NUM";
	
	INSERT INTO #UPDATE_EH_DELETION
			      ("PRM_CONTR_CUST", "ALTN_CONTR_NUM") 
		   VALUES (INT_KUNNR."PRM_CONTR_CUST", INT_KUNNR."ALTN_CONTR_NUM");
END FOR ;

--------------------------------------------------------------------------------------------------
--  Update Error Table
--------------------------------------------------------------------------------------------------

	UPDATE "NDB_INTERNET"."IFRS_R1.INTERNET.NDB_INTERNET::INT_CUST_EH" A
    FROM "NDB_INTERNET"."IFRS_R1.INTERNET.NDB_INTERNET::INT_CUST_EH" A 
    INNER JOIN #UPDATE_EH_DELETION B
    ON  A."PRM_CONTR_CUST"            		=   B."PRM_CONTR_CUST"
    AND A."ALTN_CONTR_NUM"					=   B."ALTN_CONTR_NUM"
    SET (DELETE_DATE, DELETED)      =   (CURRENT_DATE, 1);

--------------------------------------------------------------------------------------------------
-- Set Overall Status
-------------------------------------------------------------------------------------------------- 

--  STATUS 0: Send the success message to scheduling tool to confirm this procedure has been
--  executed successfully
    IF(LV_ERROR_FLAG = 'Y') THEN		

       OT_STATUS := LC_ERROR_TEXT;
			
    ELSE
 
  	   OT_STATUS := LC_SUCCESS_TEXT; 	
				
	END IF;

END; 
 