-- Update Landing Table IFRS_R1.LAND::MDM_CUST_OFFR
   UPDATE "LAND"."IFRS_R1.LAND::P77_CUST_MSTR" A
   FROM "LAND"."IFRS_R1.LAND::P77_CUST_MSTR" A 
   INNER JOIN #UPDATE_P77_CUSTOMER_MASTER B 
   ON A."CUST"              =   B."CUST"
   AND A."DIST_CHNNL"       =   B."DIST_CHNNL"
   AND A."PTNR_FNC"         =   B."PTNR_FNC"
   AND A."CUST2"            =   B."CUST2"
   AND A."CYCL_ID"          =   B."CYCLE_NUM"
   SET A.NDB_TS            = LV_CUR_UTC
   WHERE A.NDB_TS IS NULL;

--------------------------------------------------------------------------------------------------
-- Update Error Table
--------------------------------------------------------------------------------------------------

UPDATE "NDB_COMMON"."IFRS_R1.COMMON_COMP.NDB_COMMON::P77_CUSTOMER_MASTER_EH" A
