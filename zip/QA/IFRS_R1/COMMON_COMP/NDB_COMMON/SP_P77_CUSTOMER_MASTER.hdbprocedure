  INNER JOIN #UPDATE_EH_DELETION B
    ON A."CUST"              = B."CUST"
    AND A."DIST_CHNNL"       = B."DIST_CHNNL"
    AND A."PTNR_FNC"         = B."PTNR_FNC"
    AND A."CUST2"            = B."CUST2"
  SET (DELETE_DATE, DELETED) = (CURRENT_DATE, 1);

