                        WHEN  &quot;FEATURE_CODE&quot; IN ('ACT', 'CCC35') THEN 'ACT'&#xD;
                        WHEN &quot;FTR_REVENUE_CODE&quot; = 'O' THEN 'OTC'&#xD;
